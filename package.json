{
  "name": "react-web-app-boilerplate",
  "version": "0.0.0",
  "description": "React + Webpack + Babel web application boilerplate",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint --ext .js --ext .jsx .",
    "test": "npm run lint && karma start",
    "tdd": "karma start --auto-watch --no-single-run",
    "build": "webpack --config webpack.config.js --bail -p",
    "dev": "open http://localhost:8080 && webpack-dev-server --history-api-fallback --hot --inline --progress",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexKVal/react-web-app-boilerplate.git"
  },
  "keywords": [
    "application",
    "boilerplate",
    "react",
    "webpack",
    "babel"
  ],
  "author": "@alexkval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexKVal/react-web-app-boilerplate/issues"
  },
  "homepage": "https://github.com/AlexKVal/react-web-app-boilerplate#readme",
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "chai": "^3.2.0",
    "css-loader": "^0.15.6",
    "es5-shim": "^4.1.10",
    "eslint": "^0.24.1",
    "eslint-config-standard": "^3.4.1",
    "eslint-config-standard-react": "^1.0.2",
    "eslint-plugin-react": "^3.1.0",
    "eslint-plugin-standard": "^1.1.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.0",
    "isparta-instrumenter-loader": "^0.2.1",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.4",
    "karma-sinon-chai": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "mt-changelog": "^0.6.1",
    "normalize.css": "^3.0.3",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.5.1",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.8",
    "release-script": "^0.2.1",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^3.16.1"
  }
}
